{
  "githubIssues.queries": [
    {
      "label": "My App Issues",
      "query": "is:open assignee:${user} repo:WebNaresh/abhyasika_turbo_repo sort:created-desc"
    }
  ],
  "java.configuration.updateBuildConfiguration": "interactive",
  "kiroAgent.configureMCP": "Disabled",
  "search.exclude": {
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/coverage": true,
    "**/.turbo": true,
    "**/out": true,
    "**/generated": true
  },
  "terminal.integrated.defaultProfile.windows": "Command Prompt"
}
